<% unless klass.method_list.empty? then %>
<!-- Method Quickref -->
<nav id="method-list-section">
  <header>
    <h3>Methods</h3>
  </header>

  <ul class="link-list">
    <% klass.each_method do |meth| %>
    <li <% if meth.calls_super %>class="calls-super" <% end %>><a href="#<%= meth.aref %>"><%= meth.singleton ? '::' : '#' %><%= h meth.name %></a>
    <% end %>
  </ul>
</nav>
<% end %>
